{
    "name": "whitecube/laravel-flexible-content",
    "description": "Base library for repeated layout fields, content builders and other collection components",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "laravel/pint": "^1.21"
    },
    "require-dev": {
        "pestphp/pest": "^2.0|^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Whitecube\\LaravelFlexibleContent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Toon Van den Bos",
            "email": "toon@whitecube.be"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
